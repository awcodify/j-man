app:
  server:
    host: "localhost"
    port: 8888
  jmeter:
    path: bin/jmeter
DB:
  dsn: "user=jmanager_user password=password dbname=jmanager sslmode=disable"
HTML:
  root: "app/view/html/"
  layout:
    root: "app/view/html/layout"
    base_html: "app/view/html/layout/base.html"
test:
    dialect: postgres
    datasource: dbname=jmanager user=jmanager_user password=password sslmode=disable
    dir: database/migrations
    table: migrations

